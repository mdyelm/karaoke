<h3 class="page-title">Detail contact</h3>
<div class="row">
    <div class="col-sm-push-2 col-md-8">
        <!-- INPUTS -->
        <div class="panel panel-headline">
            <div class="panel-heading">
                <div class="content-detail"> 
                    <img src="<%= datas.user_id.avatar %>" style="width: 200px" class="img-circle">
                    <h3 class="name"><% if(datas.user_id.username != null) {%> <%= datas.user_id.username %> <% } %></h3>
                </div>

            </div>
            <div class="panel-body">
                <h4>singer</h4>
                <p><% if(datas.singer_name != null) {%> <%= datas.singer_name %> <% } %></p>
                <h4>song name</h4>
                <p><% if(datas.song_name != null) {%> <%= datas.song_name %> <% } %></p>
                <h4>Content</h4>
                <p><% if(datas.content != null) {%> <%= datas.content %> <% } %></p>
            </div>

        </div>
        <!-- END INPUTS -->


    </div>
</div>
<script type="text/javascript">
    console.log(<%- JSON.stringify(datas) %>);
</script>